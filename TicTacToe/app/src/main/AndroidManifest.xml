        <activity android:name="com.SRCStudios.tictactoe.P2GameActivity" />
        <activity android:name="com.SRCStudios.tictactoe.HelpActivity" />
        <activity android:name="com.SRCStudios.tictactoe.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
