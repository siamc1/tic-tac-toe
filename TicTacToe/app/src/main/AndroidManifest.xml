        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.SRCStudios.tictactoe.DifficultySelect"></activity>
        <activity android:name="com.SRCStudios.tictactoe.TieActivity" />
        <activity android:name="com.SRCStudios.tictactoe.TieActivityP2" />
